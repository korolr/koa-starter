{
  "name": "koa-starter",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "migrate:latest": "knex migrate:latest",
    "migrate:rollback": "knex migrate:rollback",
    "seed:run": "knex seed:run",
    "lint": "eslint src",
    "precommit": "lint-staged",
    "test": "jest src --runInBand --noStackTrace --forceExit",
    "test:watch": "npm run test -- --watch",
    "reset": "rm -rf node_modules/ && npm cache clean && npm prune && yarn cache clean",
    "start-dev": "nodemon src/index.js"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "1.0.2",
    "dotenv": "4.0.0",
    "knex": "0.13.0",
    "koa": "2.2.0",
    "koa-body": "2.0.1",
    "koa-bouncer": "6.0.0",
    "koa-helmet": "3.2.0",
    "koa-logger": "3.0.0",
    "koa-router": "7.2.0",
    "koa-send": "4.1.0",
    "pg": "6.2.3",
    "request": "2.81.0",
    "request-promise": "4.2.1",
    "uuid": "3.0.1",
    "validator": "7.0.0"
  },
  "devDependencies": {
    "babel-jest": "20.0.3",
    "eslint": "3.19.0",
    "eslint-plugin-react-app": "1.0.2",
    "flow-bin": "0.47.0",
    "husky": "0.13.4",
    "jest": "20.0.4",
    "lint-staged": "3.5.1",
    "nodemon": "1.11.0",
    "prettier": "1.3.1",
    "supertest": "3.0.0"
  },
  "lint-staged": {
    "src/**/*.js": [
      "prettier --write --print-width 100 --single-quote --trailing-comma es5",
      "git add"
    ]
  }
}
